{
  "expo": {
    "name": "QuackFit",
    "slug": "QuackFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "quackfit",
    "userInterfaceStyle": "automatic",    
    "ios": {
      "supportsTablet": true
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
